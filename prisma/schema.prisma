// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}


model Personaje{
  id String @id @default(uuid())
  nombre String 
  rol String @unique
  descripcion String?
  pago Int 
  capitulo Capitulo[]
}

model Capitulo{
  id String @id @default(uuid())
  nombre String
  sipnosis String
  pago String
  numCap Int 
  webhook String
  personajes Personaje[]
}